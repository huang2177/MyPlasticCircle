<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_gray_divider_out" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out" />

    <ImageView
        android:id="@+id/img_vip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <View
        android:id="@+id/vip_divider"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent">

        <TextView
            android:id="@+id/wd_text_dd"
            style="@style/myself_text_style"
            android:layout_marginLeft="8dp"
            android:drawableLeft="@drawable/icon_trade"
            android:padding="10dp"
            android:text=" 交易订单"
            android:textColor="@color/color_title_main" />

        <com.myplas.q.common.view.DragView
            android:id="@+id/order_dragview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="3dp"
            android:visibility="gone" />

        <TextView
            style="@style/myself_text_style"
            android:layout_marginRight="10dp"
            android:layout_weight="0"
            android:drawableRight="@drawable/icon_more" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_gray_divider_out" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/wd_linear_gj"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                style="@style/myself_text_style"
                android:layout_marginLeft="8dp"
                android:drawableLeft="@drawable/icon_my_supply"
                android:padding="10dp"
                android:text=" 我的供给"
                android:textColor="@color/color_title_main" />

            <TextView
                android:id="@+id/wd_text_gj"
                style="@style/myself_text_style"
                android:layout_marginRight="10dp"
                android:layout_weight="0"
                android:drawablePadding="3dp"
                android:drawableRight="@drawable/icon_more" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/color_gray_divider_in" />

        <LinearLayout
            android:id="@+id/wd_linear_qg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                style="@style/myself_text_style"
                android:layout_marginLeft="8dp"
                android:drawableLeft="@drawable/icon_my_purchase"
                android:padding="10dp"
                android:text=" 我的求购"
                android:textColor="@color/color_title_main" />

            <TextView
                android:id="@+id/wd_text_qg"
                style="@style/myself_text_style"
                android:layout_marginRight="10dp"
                android:layout_weight="0"
                android:drawablePadding="3dp"
                android:drawableRight="@drawable/icon_more" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_gray_divider_out" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out" />

    <LinearLayout
        android:id="@+id/wd_text_store"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent">

        <TextView
            style="@style/myself_text_style"
            android:layout_marginLeft="8dp"
            android:drawableLeft="@drawable/icon_my_store"
            android:padding="10dp"
            android:text=" 我的店铺"
            android:textColor="@color/color_title_main" />


        <TextView
            style="@style/myself_text_style"
            android:layout_marginRight="10dp"
            android:layout_weight="0"
            android:drawableRight="@drawable/icon_more" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_gray_divider_out" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out" />


    <LinearLayout
        android:id="@+id/wd_linear_jf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:drawableLeft="@drawable/icon_integral"
            android:gravity="center_vertical"
            android:padding="10dp"
            android:text=" 我的塑豆"
            android:textColor="@color/color_title_main"
            android:textSize="16.0sp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="top"
            android:text=" hot!"
            android:textColor="#f4410a"
            android:textSize="16.0sp"
            android:textStyle="italic" />


        <TextView
            style="@style/myself_text_style"
            android:layout_marginRight="10dp"
            android:layout_weight="0"
            android:drawableRight="@drawable/icon_more"
            android:text="  " />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_gray_divider_out" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out" />


    <LinearLayout
        android:id="@+id/wd_linear_look"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent">

        <TextView
            android:id="@+id/wd_text_look"
            style="@style/myself_text_style"
            android:layout_marginLeft="8dp"
            android:drawableLeft="@drawable/icon_who_see_me"
            android:padding="10dp"
            android:text=" 谁看过我"
            android:textColor="@color/color_title_main" />

        <com.myplas.q.common.view.DragView
            android:id="@+id/look_dragview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="3dp"
            android:visibility="gone" />

        <TextView
            style="@style/myself_text_style"
            android:layout_marginRight="10dp"
            android:layout_weight="0"
            android:drawableRight="@drawable/icon_more" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_gray_divider_out" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/wd_text_edu"
                style="@style/myself_text_style"
                android:layout_marginLeft="8dp"
                android:drawableLeft="@drawable/icon_card"
                android:padding="10dp"
                android:text=" 企业信用额度"
                android:textColor="@color/color_title_main" />

            <TextView
                style="@style/myself_text_style"
                android:layout_marginRight="10dp"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  " />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/color_gray_divider_in" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/wd_text_pz"
                style="@style/myself_text_style"
                android:layout_marginLeft="8dp"
                android:drawableLeft="@drawable/icon_with_capital"
                android:padding="10dp"
                android:text=" 塑料配资"
                android:textColor="@color/color_title_main" />

            <TextView
                style="@style/myself_text_style"
                android:layout_marginRight="10dp"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more" />
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_gray_divider_out" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="8dp"
        android:background="@color/color_gray_divider_out" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent">

        <TextView
            android:id="@+id/wd_text_set"
            style="@style/myself_text_style"
            android:layout_marginLeft="8dp"
            android:layout_weight="1"
            android:drawableLeft="@drawable/icon_setup"
            android:padding="10dp"
            android:text=" 设置"
            android:textColor="@color/color_title_main" />

        <TextView
            style="@style/myself_text_style"
            android:layout_marginRight="10dp"
            android:layout_weight="0"
            android:drawableRight="@drawable/icon_more" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginBottom="20dp"
        android:background="@color/color_gray_divider_out" />

</LinearLayout>
